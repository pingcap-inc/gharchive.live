{
  "cacheHours": 1,
  "params": [
    {
      "name": "repoId",
      "replaces": "${repoId}",
      "pattern": "^[1-9]\\d*$"
    },
    {
      "name": "from",
      "replaces": "${from}",
      "pattern": "^\\d{4}-\\d{2}-\\d{2}$",
      "default": "2000-01-01"
    },
    {
      "name": "to",
      "replaces": "${to}",
      "pattern": "^\\d{4}-\\d{2}-\\d{2}$",
      "default": "2099-12-31"
    }
  ]
}
